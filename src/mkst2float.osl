// A barebones example showing how to access 'st' texture
// coordinates and how to output a float.
shader mkst2float(
    float s = 0
        [[
          int lockgeom = 0,
          string widget = "null",
        ]],
    float t = 0
        [[
          int lockgeom = 0,
          string widget = "null",
        ]],
    output float resultF = 0)
{
  resultF = s * t;
}
